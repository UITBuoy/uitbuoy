{
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": "../",
    "roots": ["../"],
    "modulePaths": ["./"],
    "testEnvironment": "node",
    "testRegex": ".e2e-spec.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    }, 
    "preset": "ts-jest",
    "moduleNameMapper": {
        "^@/(.*)$": "<rootDir>/src/$1",
        "^src/(.*)$": "<rootDir>/src/$1"
    }
}
